{
  "schemaVersion": 1,
  "id": "weathersync",
  "version": "${version}",
  "name": "WeatherSync",
  "description": "Syncs your server's players' weather with the real world!",
  "authors": ["macbrayne"],
  "contact": {
    "sources": "https://github.com/macbrayne/weathersync",
    "issues": "https://github.com/macbrayne/weathersync/issues"
  },
  "license": "EUPL-1.2",
  "icon": "assets/weathersync/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.macbrayne.fabric.weathersync.WeatherSync"
    ],
    "cardinal-components": [
      "de.macbrayne.fabric.weathersync.components.Components"
    ]
  },
  "mixins": [
    "weathersync.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },

  "custom": {
    "cardinal-components": [
      "weathersync:location"
    ]
  }
}
